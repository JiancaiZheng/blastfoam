{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "main",
    {
      "name": "develop",
      "prerelease": true
    },
    {
      "name": "staging",
      "prerelease": true
    },
    {
      "name": "+([0-9])?-*",
      "prerelease": "${name}"
    }
  ],
  "plugins": [
    "@semantic-release/commit-analyzer"
  ]
}
