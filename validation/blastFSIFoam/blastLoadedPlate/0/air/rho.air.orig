FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho.air.orig;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.225;

boundaryField
{
    outlet
    {
        type            zeroGradient;
    }

    ground
    {
        type            zeroGradient;
    }

    air_to_plate
    {
        type            zeroGradient;
    }

    wedge
    {
        type            wedge;
    }

    symmetry
    {
        type            symmetry;
    }
}

