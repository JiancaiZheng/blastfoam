#include "$FOAM_CASE/geometry_3D"

blocks
(
    hex (7 8 20 19 11 12 24 23) ($nw $ny $nw ) simpleGrading (1 1 1)
    hex (8 9 21 20 12 13 25 24) ($nr $ny $nw) simpleGrading (1 1 1)
    hex (9 10 22 21 13 14 26 25) ($nx $ny $nw) simpleGrading (1 1 1)
    hex (11 12 24 23 15 13 25 27) ($nw $ny $nr) simpleGrading (1 1 1)
    hex (15 13 25 27 16 17 29 28) ($nw $ny $nx) simpleGrading (1 1 1)
    hex (13 14 26 25 17 18 30 29) ($nx $ny $nx) simpleGrading (1 1 1)
);

edges
(
    arc 9 13 ($xRm $y1 $xRp)
    arc 21 25 ($xRm $y2 $xRp)

    arc 13 15 ($xRp $y1 $xRm)
    arc 25 27 ($xRp $y2 $xRm)
);

boundary
(
    outlet
    {
        type patch;
        faces
        (
            (10 14 26 22)
            (14 18 30 26)
            (18 17 29 30)
            (17 16 28 29)
            (19 20 24 23)
            (20 21 25 24)
            (21 22 26 25)
            (23 24 25 27)
            (27 25 29 28)
            (25 26 30 29)
        );
    }
    ground
    {
        type wall;
        faces
        (
            (9 10 14 13)
            (15 13 17 16)
            (13 14 18 17)
        );
    }
    air_to_plate
    {
        type patch;
        sampleMode nearestPatchFace;
        sampleRegion plate;
        samplePatch plate_to_air;
        faces
        (
            (7 8 12 11)
            (8 9 13 12)
            (11 12 13 15)
        );
    }
    symmetry0
    {
        type symmetry;
        faces
        (
            (7 8 20 19)
            (8 9 21 20)
            (9 10 22 21)
        );
    }
    symmetry1
    {
        type symmetry;
        faces
        (
            (7 11 23 19)
            (11 15 27 23)
            (15 16 28 27)
        );
    }
);

mergePatchPairs
(
