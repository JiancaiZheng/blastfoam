(
    outlet
    {
        type patch;
        faces
        (
            (4 7 12 10)
            (7 6 11 12)
            (6 5 5 11)
        );
    }
    ground
    {
        type wall;
        faces
        (
            (3 4 10 9)
        );
    }
    air_to_plate
    {
        type patch;
        sampleMode nearestPatchFace;
        sampleRegion plate;
        samplePatch plate_to_air;
        faces
        (
            (2 3 9 2)
        );
    }
    wedge0
    {
        type wedge;
        faces
        (
            (2 3 6 5)
            (3 4 7 6)
        );
    }
    wedge1
    {
        type wedge;
        faces
        (
            (2 9 11 5)
            (9 10 12 11)
        );
    }
