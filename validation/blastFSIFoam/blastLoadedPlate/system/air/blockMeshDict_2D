blocks
(
    hex (2 3 6 5 2 9 11 5) ($nx0 $ny 1 ) simpleGrading (1 1 1)
    hex (3 4 7 6 9 10 12 11) ($nx1 $ny 1) simpleGrading (1 1 1)
);

edges
(
);

