FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     blastFSIFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.01;

deltaT          1e-7;

writeControl    adjustableRunTime;

runTimeModifiable yes;

writeInterval   2e-5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   6;

maxCo 0.5;

adjustTimeStep yes;

#include "../properties"
DebugSwitches
{
    meshSizeObject 1;
    errorEstimator 1;
}

functions
{
    plateProbes
    {
        type blastProbes;
        adjustLocations yes;
        region plate;

        probeLocations ((0 $nthickness 0));
        fields (U D);
    }
}
