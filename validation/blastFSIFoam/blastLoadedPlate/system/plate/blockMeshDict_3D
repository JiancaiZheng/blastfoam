FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../../geometry_3D"

blocks
(
    hex (0 1 3 2 8 9 11 10) ($nxPlate $nyPlate $nxPlate) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    plate_to_air
    {
        type patch;
        sampleMode nearestPatchFace;
        sampleRegion air;
        samplePatch air_to_plate;
        faces
        (
            (2 3 11 10)
        );
    }
    plateEnd
    {
        type patch;
        faces
        (
            (1 3 11 9)
            (8 9 11 10)
        );
    }
    plateFree
    {
        type patch;
        faces
        (
            (0 1 9 8)
        );
    }
    symmetry0
    {
        type symmetry;
        faces
        (
             (0 1 3 2)
        );
    }
    symmetry1
    {
        type symmetry;
        faces
        (
             (0 8 10 2)
        );
    }
);

mergePatchPairs
(
);


