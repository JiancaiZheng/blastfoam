FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
scale 1.0;

#include "$FOAM_CASE/geometry_3D"

blocks
(
    hex (0 1 8 7 3 4 12 11) ($nwPlate $nyPlate $nwPlate) simpleGrading (1 1 1)
    hex (1 2 9 8 4 5 13 12) ($nrPlate $nyPlate $nwPlate) simpleGrading (1 1 1)
    hex (3 4 12 11 6 5 13 15) ($nwPlate $nyPlate $nrPlate) simpleGrading (1 1 1)
);

edges
(
    arc 2 5 ($xRm $y0 $xRp)
    arc 9 13 ($xRm $y1 $xRp)

    arc 5 6 ($xRp $y0 $xRm)
    arc 13 15 ($xRp $y1 $xRm)
);

boundary
(
    plate_to_air
    {
        type patch;
        sampleMode nearestPatchFace;
        sampleRegion air;
        samplePatch air_to_plate;
        faces
        (
            (7 8 12 11)
            (8 9 13 12)
            (11 12 13 15)
        );
    }
    plateEnd
    {
        type patch;
        faces
        (
            (2 5 13 9)
            (5 6 15 13)
        );
    }
    plateFree
    {
        type patch;
        faces
        (
            (0 1 4 3)
            (1 2 5 4)
            (3 4 5 6)
        );
    }
    symmetry0
    {
        type symmetry;
        faces
        (
            (0 1 8 7)
            (1 2 9 8)
        );
    }
    symmetry1
    {
        type symmetry;
        faces
        (
             (0 3 11 7)
             (3 6 15 11)
        );
    }
);

mergePatchPairs
(
);


