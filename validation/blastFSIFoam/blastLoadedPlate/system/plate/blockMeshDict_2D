FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
scale 1.0;

#include "../../geometry_2D"

blocks
(
    hex (0 1 3 2 0 8 9 2) ($nxPlate $nyPlate 1) simpleGrading (10 1 1)
);

edges
(
);

boundary
(
    plate_to_air
    {
        type patch;
        sampleMode nearestPatchFace;
        sampleRegion air;
        samplePatch air_to_plate;
        faces
        (
            (2 3 9 2)
        );
    }
    plateEnd
    {
        type patch;
        faces
        (
             (1 3 9 8)
        );
    }
    plateFree
    {
        type patch;
        faces
        (
             (0 1 8 0)
        );
    }
    wedge0
    {
        type wedge;
        faces
        (
             (0 1 3 2)
        );
    }
    wedge1
    {
        type wedge;
        faces
        (
             (0 8 9 2)
        );
    }
);

mergePatchPairs
(
);


