FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    bottomFixedWalls
    {
        type            fixedNormalSlip;
        n               (0 -1 0);
    }
    topFixedWalls
    {
        type            fixedNormalSlip;
        n               (0 1 0);
    }
    fluid_to_walls
    {
        type            mapInterpolated;
        volField        D;
        value           uniform (0 0 0);
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}

// ************************************************************************* //
