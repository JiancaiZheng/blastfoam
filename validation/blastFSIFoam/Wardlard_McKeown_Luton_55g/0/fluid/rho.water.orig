FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      rho.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1000;

boundaryField
{
    fixedWalls
    {
        type            zeroGradient;
    }
    fluid_to_walls
    {
        type            zeroGradient;
    }
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
}

