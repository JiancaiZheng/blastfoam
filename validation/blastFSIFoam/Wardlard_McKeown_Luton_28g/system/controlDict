FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     blastFSIFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         400e-6;

deltaT          1e-8;

writeControl    adjustableRunTime;

writeInterval   1e-6;

cycleWrite      0;

writeFormat     binary;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.5;

maxDeltaT       1;

// DebugSwitches
// {
//     globalPolyPatch 1;
// }
// ************************************************************************* //

functions
{
    solidProbes
    {
        type probes;
        region walls;
        fixLocations yes;
        append yes;
        probeLocations
        (
            (0.047625 0.115 0)
        );
        fields (D U);
    }
    conservedQuantities
    {
        type    conservedQuantities;
        fields  (rhoE);
        region  fluid;
        writeControl timeStep;
    }
}



