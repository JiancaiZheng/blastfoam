FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system/walls";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

d2dt2Schemes
{
    default         Euler;
}

ddtSchemes
{
    default         Euler;
}

divSchemes
{
    default none;
    div(sigma)     Gauss linear;
    div((impK*grad(D))) Gauss linear;
    div(sigmaD)    Gauss linear;
    div((impK*grad(DD))) Gauss linear;
}

gradSchemes
{
    default         extendedLeastSquares 0;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}


