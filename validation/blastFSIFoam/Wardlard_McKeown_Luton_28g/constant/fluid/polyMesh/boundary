FoamFile
{
    format      binary;
    class       polyBoundaryMesh;
    location    "constant/fluid/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

6
(
    bottomFixedWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          50;
        startFace       19750;
    }
    topFixedWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          50;
        startFace       19800;
    }
    fluid_to_walls
    {
        type            mappedMovingWall;
        nFaces          200;
        startFace       19850;
        sampleMode      nearestPatchFace;
        sampleDisplacementField D;
        sampleRegion    walls;
        samplePatch     walls_to_fluid;
    }
    front
    {
        type            wedge;
        inGroups        List<word> 1(wedge);
        nFaces          10000;
        startFace       20050;
    }
    back
    {
        type            wedge;
        inGroups        List<word> 1(wedge);
        nFaces          10000;
        startFace       30050;
    }
    defaultFaces
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          0;
        startFace       40050;
    }
)

