FoamFile
{
    format      binary;
    class       polyBoundaryMesh;
    location    "constant/walls/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

5
(
    fixedWalls
    {
        type            wall;
        inGroups        List<word> 1(wall);
        nFaces          40;
        startFace       7780;
    }
    freeWall
    {
        type            patch;
        nFaces          200;
        startFace       7820;
    }
    walls_to_fluid
    {
        type            mappedMovingWall;
        nFaces          200;
        startFace       8020;
        sampleMode      nearestPatchFace;
        displacementField pointD;
        sampleRegion    fluid;
        samplePatch     fluid_to_walls;
    }
    front
    {
        type            wedge;
        inGroups        List<word> 1(wedge);
        nFaces          4000;
        startFace       8220;
    }
    back
    {
        type            wedge;
        inGroups        List<word> 1(wedge);
        nFaces          4000;
        startFace       12220;
    }
)

