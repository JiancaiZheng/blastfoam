FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
scale 1;

#include "$FOAM_CASE/geometry_2D"

vertices
(
    ($x0  $y0  $nz0)         // 0
    ($x1  $y0  $nz1)         // 1
    ($x0  $y1  $nz0)         // 2
    ($x1  $y1  $nz1)         // 3
    ($x2  $y1  $nz2)         // 4
    ($x0  $y2  $nz0)         // 5
    ($x1  $y2  $nz1)         // 6
    ($x2  $y2  $nz2)         // 7

    ($x1  $y0  $z1)         // 8
    ($x1  $y1  $z1)         // 9
    ($x2  $y1  $z2)         // 10
    ($x1  $y2  $z1)         // 11
    ($x2  $y2  $z2)         // 12
);

blocks
(
    hex (0 1 3 2 0 8 9 2) (100 5 1) simpleGrading (10 1 1)
);

edges
(
);

boundary
(
    plate_to_air
    {
        type patch;
        faces
        (
            (2 3 9 2)
        );
    }
    plateEnd
    {
        type patch;
        faces
        (
             (1 3 9 8)
        );
    }
    plateFree
    {
        type patch;
        faces
        (
             (0 1 8 0)
        );
    }
    wedge0
    {
        type wedge;
        faces
        (
             (0 1 3 2)
        );
    }
    wedge1
    {
        type wedge;
        faces
        (
             (0 8 9 2)
        );
    }
);

mergePatchPairs
(
);


