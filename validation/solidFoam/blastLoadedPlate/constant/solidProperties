FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solidModel explicitNonLinearGeometryTotalLagrangian;
// solidModel nonLinearGeometryTotalLagrangian;

explicitNonLinearGeometryTotalLagrangianCoeffs
{
    incompressiblilityCoefficient   1.0;
    angularMomentumConservation     yes;
}


nonLinearGeometryTotalLagrangianCoeffs
{
    // Maximum number of momentum correctors
    nCorrectors             2000;

    // Solution tolerance for displacement
    solutionTolerance       1e-04;

    // Alternative solution tolerance for displacement
    alternativeTolerance    1e-06;

    // Material law solution tolerance
    materialTolerance       1e-06;

    // Write frequency for the residuals
    infoFrequency           100;
}


