FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// solidModel explicitLinear;
solidModel explicitNonLinearTotalLagrangian;
// solidModel linear;
// solidModel nonLinearTotalLagrangian;

"explicit.*"
{
    JSTScaleFactor 0.0001;
    LFScaleFactor 0.0001;

    incompressiblilityCoefficient   1;
    angularMomentumConservation     yes;
}


"(unsLinear|linear|unsNonLinear|nonLinear).*"
{
    // Maximum number of momentum correctors
    nCorrectors             1000;

    // Solution tolerance for displacement
    solutionTolerance       1e-04;

    // Alternative solution tolerance for displacement
    alternativeTolerance    1e-06;

    // Material law solution tolerance
    materialTolerance       1e-06;

    // Write frequency for the residuals
    infoFrequency           100;
}


