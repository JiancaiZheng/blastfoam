FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      D;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            KBSolidTraction;
        pRef            uniform 0;
        traction        uniform (0 0 0);
        pressure        uniform 0;
        value           uniform (0 0 0);

        KBSolver        CONWEP;
        burstType       free;
        centre          (0 0.5 0);
        mass            50;
    }
    fixed
    {
        type            fixedDisplacement;
        value           uniform (0 0 0);
    }
    free
    {
        type            solidTractionFree;
        traction        uniform ( 0 0 0 );
        pressure        uniform 0;
        value           uniform (0 0 0);
    }
}


