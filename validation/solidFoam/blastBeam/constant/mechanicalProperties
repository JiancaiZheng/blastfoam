FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     yes;

mechanical
(
    steel
    {
        type            linearElastic;
        E               28e9;
        nu              0.28;
        sigmaY          3.4667e7;

        solvePressureEqn no;
    }
);

