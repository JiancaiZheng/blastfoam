FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fields (alpha.air);
nBufferLayers 0;

defaultFieldValues
(
    volScalarFieldValue p           1e5
    volScalarFieldValue alpha.air   1
    volScalarFieldValue rho.air     1.4
    volVectorFieldValue U           (0 0 0)
);


regions
(
    cylinderToCell
    {
        p1 (0.1335 0 -1);
        p2 (0.1335 0 1);
        radius 0.025;
        level 3;

        fieldValues
        (
            volScalarFieldValue alpha.air   0
        );
    }

    boxToCell
    {
        boxes ((0 -2.5 -1) (0.1 2.5 1));
        level 3;

        fieldValues
        (
            volScalarFieldValue p           156980
            volScalarFieldValue rho.air     1.92691
            volVectorFieldValue U           (114.42 0 0)
        );
    }
);


