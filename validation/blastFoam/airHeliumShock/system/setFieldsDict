FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fields (alpha.air);
nBufferLayers 2;

defaultFieldValues
(
    volScalarFieldValue p           1e5
    volScalarFieldValue rho.air     1.4
    volVectorFieldValue U           (0 0 0)
);


regions
(
    boxToCell
    {
        boxes ((0 -2.5 -1) (0.1001 2.5 1));

        fieldValues
        (
            volScalarFieldValue p           156980
            volScalarFieldValue rho.air     1.92691
            volVectorFieldValue U           (114.42 0 0)
        );
    }
);


