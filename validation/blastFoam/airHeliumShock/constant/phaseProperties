FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (air helium);
systemType interface;

sigma
{
    type constant;
    sigma 0.07;
}
air
{
    smoothPow 0.1;
    epsilon 0.75;
    truncation tanh;
//     levelSetFunction exp;

    defaultValue 1.0;
    initialRegions
    (
        searchableCylinder
        {
            point1 (0.1335 0 -1);
            point2 (0.1335 0 1);
            radius 0.025;
            level 3;
            remove yes;
        }
    );


    type basic;
    thermoType
    {
        transport   const;
        thermo      eConst;
        equationOfState stiffenedGas;
    }
    equationOfState
    {
        gamma           1.4;
        a               0;
    }
    specie
    {
        molWeight       28.97;
    }
    transport
    {
        mu              0;              // Viscosity
        Pr              1;              // Prandtl number
    }
    thermodynamics
    {
        Cv          718;           // Heat capacity
        Hf          0.0;
    }

    residualRho     1e-6;
    residualAlpha   1e-6;
}

helium
{
    type basic;
    thermoType
    {
        transport   const;
        thermo      eConst;
        equationOfState stiffenedGas;
    }
    equationOfState
    {
        gamma           1.648;
        a               0;
    }
    specie
    {
        molWeight       2;
    }
    transport
    {
        mu              0;              // Viscosity
        Pr              1;              // Prandtl number
    }
    thermodynamics
    {
        Cv          3115.6;           // Heat capacity
        Hf          0.0;
    }

    residualRho     1e-6;
    residualAlpha   1e-6;
}



