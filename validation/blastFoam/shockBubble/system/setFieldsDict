FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fields (alpha.air);
nBufferLayers 0;

defaultFieldValues
(
    volScalarFieldValue p           1e5
    volScalarFieldValue alpha.air   0
    volScalarFieldValue alpha.water 1
    volScalarFieldValue rho.water   1000
    volVectorFieldValue U           (0 0 0)
);


regions
(
    cylinderToCell
    {
        p1 (0.012 0 -1);
        p2 (0.012 0 1);
        radius 0.006;
        level 3;

        fieldValues
        (
            volScalarFieldValue alpha.air   1
            volScalarFieldValue alpha.water 0
        );
    }

    boxToCell
    {
        boxes ((0 -2.5 -1) (0.005 2.5 1));
        level 3;

        fieldValues
        (
            volScalarFieldValue p           1.9e9
            volScalarFieldValue rho.water   1323.65
            volVectorFieldValue U           (681.58 0 0)
        );
    }
);


// ************************************************************************* //
