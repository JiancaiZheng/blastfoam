
table1D
{
    file "T.csv";
    eMod ln;
    TMod ln;
    interpolationScheme cubicClamp;
    isReal false;
}

table2D
{
    file        "p.csv";
    mod          ln;
    delim       ";";
    isReal      false;

    rhoCoeffs
    {
        mod         log10;

        n           7;
        delta       1.0;
        min         -3.0;
    }
    eCoeffs
    {
        mod         ln;

        n           40;
        min         11.8748;
        delta       1.0;
    }
}

table3D
{
    fCoeffs
    {
        file        "table3D.csv";
        mod         none;
        delim       ",";
        rowDelim    ";";
        isReal      true;
    }

    xCoeffs
    {
        mod         none;
        n           5;
        delta       0.5;
        min         0.0;
    }
    yCoeffs
    {
        mod         none;
        n           7;
        delta       0.5;
        min         0.0;
    }
    zCoeffs
    {
        mod         none;
        n           9;
        delta       0.5;
        min         0.0;
    }
}
