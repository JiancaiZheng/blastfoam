
table1D
{
    interpolationScheme linearClamp;
    xCoeffs
    {
        file    "x.csv";
        delim ";";
    }
    fCoeffs
    {
        file    "table1D.csv";
    }
}

table2D
{
    interpolationScheme linearClamp;
    xCoeffs
    {
        file        "x.csv";
        delim       ";";
    }
    yCoeffs
    {
        file        "y.csv";
        delim       ";";
        mod         ln;
        isReal      yes;
    }
    fCoeffs
    {
        file        "table2D.csv";
        mod         pow10;
        isReal      yes;
    }
}

table3D
{
    interpolationScheme quadraticClamp;
    fCoeffs
    {
        file        "table3D.csv";
        mod         none;
        delim       ",";
        rowDelim    ";";
        mod         ln;
        isReal      yes;
    }

    xCoeffs
    {
        file        "x.csv";
        delim       ";";
        mod         exp;
        isReal      yes;

    }
    yCoeffs
    {
        file        "y.csv";
        delim       ";";
    }
    zCoeffs
    {
        file        "z.csv";
        mod         pow10;
        isReal      yes;
    }
}
