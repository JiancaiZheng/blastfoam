/*--------------------------------*- C++ -*----------------------------------*\
  =========                 |
  \\      /  F ield         | OpenFOAM: The Open Source CFD Toolbox
   \\    /   O peration     | Website:  https://openfoam.org
    \\  /    A nd           | Version:  dev
     \\/     M anipulation  |
\*---------------------------------------------------------------------------*/
FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
convertToMeters 1;

theta   #calc "2.5*Foam::constant::mathematical::pi/180.0";
r      0.05;


y       #calc "$r*cos($theta)";
z      #calc "$r*sin($theta)";
nz      #calc "-$r*sin($theta)";
vertices
(
    (-0.05 0 0)
    (0.05 0 0)

    (0.05 $y $nz)
    (-0.05 $y $nz)

    (0.05 $y $z)
    (-0.05 $y $z)

);

blocks
(
    hex (0 1 2 3 0 1 4 5) (200 100 1) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    front
    {
        type wedge;
        faces
        (
            (0 1 2 3)
        );
    }
    back
    {
        type wedge;
        faces
        (
            (0 1 4 5)
        );
    }
    inlet
    {
        type patch;
        faces
        (
            (1 2 4 1)
        );
    }
    outlet
    {
        type patch;
        faces
        (
            (0 3 5 0)
            (2 3 5 4)
        );
    }
);

mergePatchPairs
(
);
