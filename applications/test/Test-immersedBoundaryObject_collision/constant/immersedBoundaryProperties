collisionModel
{
    type pairCollision;
    pairModel pairInelastic;
    pairInelasticCoeffs
    {
        alpha   0.12;
        b       1.5;
        mu      0.52;
        nu      0.33;
        E       6e8;
        cohesionEnergyDensity 0;
    }
    wallModel wallInelastic;
    wallInelasticCoeffs
    {
        e       0.9;
        staticFrictionCoeff 0;
        dynamicFrictionCoeff 0.0;
        alpha   0.12;
        b       1.5;
        mu      0.52;
        nu      0.33;
        E       6e8;
        cohesionEnergyDensity 0;
    };
}
