    ellipse
    {
        type moving;
        shape ellipse;
        radius 2;
        centreOfMass (-6 -6 0);
        mass 2;
        write yes;
//         velocity (1 0 0);
        orientation $rotation;
        a 1;
        b 3;
        dx 0.1;

        solver
        {
            type Euler;
        }
        motionConstraints
        {
            plane
            {
                type plane;
                normal (0 0 1);
            }
        }
    }
    circle
