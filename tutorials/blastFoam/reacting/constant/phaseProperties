FoamFile
{
    format      ascii;
    class       dictionary;
    object      phaseProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type reacting;
thermoType
{
    transport sutherland;
    thermo janaf;
    equationOfState VanderWaals;
};

calculateDensity yes;
#include "thermo.compressibleGas"
defaultSpecie N2;


