FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/solid";
    object      mechanicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress     no;
thermalStress   no;
compactNormalStress no;

mechanical
(
    building
    {
        type    linearElastic;
        nu      0.3;
        E       1e9;
    }
);

