phases (coal particles0 particles1 particles2 particles3 particles4 gas);
pRef 67e3;
coal
{
    phaseType        granular;
    diameterModel constant;
    constantDiameterCoeffs
    {
        d           10e-6;
    }
    alphaMax    0.65;


    type basic;
    thermoType
    {
        equationOfState rhoConst;
        thermo          eConst;
        transport       constIso;
    }
    specie
    {
        molWeight   100.0;
    }
    equationOfState
    {
        rho0         1300;
    }
    transport
    {
        kappa       0.36;
    }
    thermodynamics
    {
        type        eConst;
        Cv          987;           // Heat capacity
        Hf          0.0;
    }

    kineticTheoryCoeffs
    {
        e       0.9;
    }
    residualRho 0;
    residualAlpha 1e-10;
}

