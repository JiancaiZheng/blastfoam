FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      c;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    sides
    {
        type            fixedValue;
        value           uniform 10;
    }
    symmetryPlanes
    {
        type            empty;
    }
}


