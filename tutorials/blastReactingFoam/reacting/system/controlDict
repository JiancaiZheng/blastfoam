FoamFile
{
  format      ascii;
  class       dictionary;
  location    "system";
  object      "controlDict";
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     blastReactingFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.5;

writeControl    adjustableRunTime;

writeInterval   0.05;

writeFormat     ascii;

writePrecision  12;

writeCompression off;

timeFormat      general;

timePrecision   12;

runTimeModifiable true;

adjustTimeStep  yes;

deltaT          1e-6;

maxCo           0.5;

