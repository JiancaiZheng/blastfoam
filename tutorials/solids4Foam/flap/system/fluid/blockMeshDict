// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

scale   1.0;

x0 -3.0;
x1 -0.05;
x2 0.05;
x3 3.0;

y0 0.0;
y1 1.0;
y2 3.0;

w 0.3;

vertices
(
    ($x0  $y0  0)         // 0
    ($x1  $y0  0)         // 1
    ($x2  $y0  0)         // 2
    ($x3  $y0  0)         // 3
    ($x0  $y1  0)         // 4
    ($x1  $y1  0)         // 5
    ($x2  $y1  0)         // 6
    ($x3  $y1  0)         // 7
    ($x0  $y2  0)         // 8
    ($x1  $y2  0)         // 9
    ($x2  $y2  0)         // 10
    ($x3  $y2  0)         // 11

    ($x0  $y0  $w)         // 12
    ($x1  $y0  $w)         // 13
    ($x2  $y0  $w)         // 14
    ($x3  $y0  $w)         // 15
    ($x0  $y1  $w)         // 16
    ($x1  $y1  $w)         // 17
    ($x2  $y1  $w)         // 18
    ($x3  $y1  $w)         // 19
    ($x0  $y2  $w)         // 20
    ($x1  $y2  $w)         // 21
    ($x2  $y2  $w)         // 22
    ($x3  $y2  $w)         // 23
);

// Grading
h1 60;
h2 2;
v1 25;
v2 50;

blocks
(
    hex (0 1 5 4 12 13 17 16) ($h1 $v1 1 ) simpleGrading (1 1 1)
    hex (2 3 7 6 14 15 19 18) ($h1 $v1 1) simpleGrading (1 1 1)
    hex (4 5 9 8 16 17 21 20) ($h1 $v2 1) simpleGrading (1 1 1)
    hex (5 6 10 9 17 18 22 21) ($h2 $v2 1) simpleGrading (1 1 1)
    hex (6 7 11 10 18 19 23 22) ($h1 $v2 1 ) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
    outlet
    {
        type patch;
        faces
        (
            (  0   4   16  12  )
            (  4   8   20  16  )
            (   3   7   19  15  )
            (   7   11  23  19  )
            (   8   9   21  20  )
            (   9   10  22  21  )
            (   10  11  23  22  )
        );
    }
    ground
    {
        type wall;
        faces
        (
            (   0   1   13  12  )
            (   2   3   15  14  )

        );
    }
    interface
    {
        type wall;
        faces
        (
            (1 5 17 13)
            (5 6 18 17)
            (2 6 18 14)
        );
    }
);

