FoamFile
{
    version     2.0;
    format      binary;
    class       dictionary;
    location    "system/ground";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    "(D|DD|T)"
    {
        solver          PCG;
        tolerance       1e-8;
        relTol          0.01;
        preconditioner  DIC;
    }
}

stressAnalysis
{
    compactNormalStress yes;
    nCorrectors     10;
    D               1e-06;
}

relaxationFactors
{
    D  0.5;
    DD 0.5;
//     T  1;
}

