FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
    // this domain is slightly larger than the bounds of the geometry in the stl
    (  0    0   0) // bottom
    (  1    0   0)
    (  1    1   0)
    (  0    1   0)
    (  0    0   1) // top
    (  1    0   1)
    (  1    1   1)
    (  0    1   1)
);

blocks
(
    hex (0 1 2 3 4 5 6 7) (10 10 1) simpleGrading (1 1 1)
);

edges
(
);

boundary
(

    patches
    {
        type patch;
        faces
        (
            (0 1 5 4)
            (1 2 6 5)
            (3 2 6 7)
            (0 3 7 4)
        );
    }
);

mergePatchPairs
(
);

